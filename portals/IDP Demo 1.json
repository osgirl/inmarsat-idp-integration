                                "CSS": "#map {\n    position:absolute;\n}",
                                "HTML": "\u003c!DOCTYPE html\u003e\n\u003cdiv id='map' class='rounded' style='width: 100%; height: 100%;'\u003e\u003c/div\u003e",
                                "JavaScript": "//JavaScript\r\nconfig = {\r\n    accessToken: 'pk.eyJ1IjoiZ2JydWNlcGF5bmUiLCJhIjoiY2o5NXRjN3k2MXJoMjM1cWJpczNneGdhcCJ9.8P1eK3BRCk58arAOwaQ-RA',\r\n    defaultzoom: 2,\r\n    center: [0, 0], //Note lat/lng are opposite of Google maps\r\n    targetContainer: \"map\"\r\n};\r\n\r\n\r\n//Load the scripts/css files\r\nif (typeof(mapboxgl) === \"undefined\") {\r\n    head.load([\"//api.mapbox.com/mapbox-gl-js/v0.41.0/mapbox-gl.css\", \"//api.mapbox.com/mapbox-gl-js/plugins/mapbox-gl-draw/v1.0.0/mapbox-gl-draw.css\"]);\r\n    head.js([\"//api.mapbox.com/mapbox-gl-js/v0.41.0/mapbox-gl.js\", \"//api.mapbox.com/mapbox-gl-js/plugins/mapbox-gl-draw/v1.0.0/mapbox-gl-draw.js\"], init);\r\n} else {\r\n    if (typeof(this.datasource)!==\"undefined\") {\r\n        //Updates for drawing markers\r\n        if (document.getElementById(config.targetContainer).innerHTML === \"\")\r\n            init();\r\n        var ds = this.datasource;\r\n        for (i=0; i \u003c ds.length; i++) {\r\n            var asset = ds[i].data;\r\n            if (asset.lat !== null \u0026\u0026 asset.lat !== 91) {\r\n                if (markerList[asset.name]) {\r\n                    markerList[asset.name].setLngLat([asset.lng, asset.lat]);\r\n                } else {\r\n                    var el = document.createElement('div');\r\n                    // create the popup\r\n                    var popup = new mapboxgl.Popup()\r\n                        .setHTML(asset.name + '\u003cbr /\u003e' + asset.timestamp + '\u003cbr /\u003e' + asset.event);\r\n                    el.style = \"background-image: url(\" + asset.asset_icon_url + \");background-size: cover;width: 16px;height: 16px;border-radius: 50%;cursor: pointer;\"; //DS.icon is for the image\r\n                    var marker = new mapboxgl.Marker(el)\r\n                        .setLngLat([asset.lng, asset.lat])\r\n                        .setPopup(popup)\r\n                        .addTo(mapObj);\r\n                    markerList[asset.name] = marker;\r\n                }\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n//Renders the maps\r\nfunction init() {\r\n    window.markerList = [];\r\n    mapboxgl.accessToken = config.accessToken;\r\n    //if (typeof(window.mapObj)===\"undefined\") {\r\n    window.mapObj = new mapboxgl.Map({\r\n        style: 'mapbox://styles/mapbox/streets-v10',\r\n        zoom: config.defaultzoom,\r\n        pitch: 0,\r\n        bearing: 0,\r\n        hash: true,\r\n        container: config.targetContainer,\r\n        center: config.center\r\n    });\r\n    //}\r\n    window.mapObj.setCenter(config.center);\r\n    window.mapObj.setZoom(config.defaultzoom);\r\n    mapObj.addControl(new mapboxgl.FullscreenControl());\r\n    mapObj.addControl(new mapboxgl.ScaleControl());\r\n}"
